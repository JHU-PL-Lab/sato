let rec reverse l ac = 
    match l with
    | [] -> ac
    | hd :: tl -> reverse tl (hd :: ac)
    end
in
let rec makeList n = 
    if n == 0 then []
    else n :: (makeList n-1)
in
let getHead l = 
    match l with
    | [] -> assert false
    | hd :: tl -> hd
    end
in
let main (n : {. int | fun a -> a >= 0}) : {. int | fun a -> a >= 0} =
    let l = makeList n in
    if n > 0 then getHead l
    else 0
in
main