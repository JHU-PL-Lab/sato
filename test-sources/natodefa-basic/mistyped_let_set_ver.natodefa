let isInt v = 
    match v with
    | int -> true
    | any -> false
    end
in
let isBool v = 
    match v with
    | bool -> true
    | any -> false
    end
in
let coinToss (x : int) : int =
    if x < 0 then true else false
in
let test = input in
let (x : bool) = 
    if (coinToss test)
    then true else 1
in
x
