# EXPECT-WELL-FORMED
# EXPECT-TYPE-ERROR-COUNT 2
# EXPECT-REQUIRED-INPUT-SEQUENCE-GENERATION-STEPS 18000
# EXPECT-TYPE-ERROR ~result [0] : "match" "n = hd" "true" "n + 1" "int" "bool"
# EXPECT-TYPE-ERROR ~result [1] : "match" "n = hd" "true" "n + 1" "int" "bool"

let rec map fn lst =
    match lst with
    | hd :: tl -> (fn hd) :: (map fn tl)
    | [] -> []
    end
in
let inc n = n + 1 in
let i = input in
if i == 0 then map inc [1, true] else map inc [true, 1]
